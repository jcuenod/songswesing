<table class="table table-condensed table-striped tableindex">
	<thead>
		<th></th>
		<th>Song</th>
		<th>#</th>
		<th>Last Used</th>
	</thead>
	<tbody>
	<% @usages.each_with_index do |usage, index| %>
		<tr>
			<td><%= index+1 %></td>
			<td><a class="unborderedanchor songAnchor" id="<%= usage.song_id %>"><%= usage.song_name %></a></td>
			<td><%= usage.count_song_id %></td>
			<td><%= usage.date %><%# .strftime "%-d %b %B %Y" %></td>
		</tr>
	<% end %>
	</tbody>
</table>